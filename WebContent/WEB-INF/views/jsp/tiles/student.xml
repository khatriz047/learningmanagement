<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	<definition name="studentpage" template="/WEB-INF/views/jsp/layouts/main.jsp">
		<put-attribute name="title" value="Student page" />
		<put-attribute name="header" type="template"
			value="/WEB-INF/views/jsp/templates/header.jsp" />
		<put-attribute name="leftmenu" type="template"
			value="/WEB-INF/views/jsp/templates/leftmenu.jsp" />
		<put-attribute name="content" type="template"
			value="/WEB-INF/views/jsp/templates/container.jsp" />
		<put-attribute name="footer" type="template"
			value="/WEB-INF/views/jsp/templates/footer.jsp" />
	</definition>
	<!-- Resourse Page -->
	<definition name="resourcestudent" extends="studentpage">
		<put-attribute name="title" value="Student resourse page" />
		<put-attribute name="content" type="template"
			value="/WEB-INF/views/jsp/templates/student/resource.jsp" />
	</definition>

</tiles-definitions>